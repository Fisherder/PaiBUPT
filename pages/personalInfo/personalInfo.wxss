/* pages/personalInfo/personalInfo.wxss */
#containerLogIn{
  display: flex; /* 使用 flexbox 布局 */
  justify-content: center; /* 水平居中 */
  align-items: center; /* 垂直居中 */
  height: 100vh; /* 高度占满视口 */
}

#logInButton{
  width: 200px; /* 设置按钮宽度 */
  height: 50px; /* 设置按钮高度 */
  font-size: 18px; /* 设置字体大小 */
  margin-top: 20px; /* 上边距 */
}